          // cookie/storage handling
          var cookieExpireDays = 365;
          var dtidCookie = Cookies.get("dtid");

          if (!initStore() && !dtidCookie) {
              getDtid();
          }

          function getDtid() {
              $.ajax("/include/ajax/dtid.php")
                  .done(function(result) {
                      if (result) {
                          if (store.enabled) store.set('dtid', result);
                      }
                  });
          }

          function initStore() {
              if (!store.enabled) {
                  return false;
              }

              var hasDtid = false;
              var dtidToken = store.get('dtid');

              if (dtidToken) {
                  Cookies.set('dtid', dtidToken, { path: '/', expires: cookieExpireDays });
                  hasDtid = true;
              } else if (dtidCookie) {
                  store.set('dtid', dtidCookie);
                  hasDtid = true;
              }

              return hasDtid;
          }
      });
